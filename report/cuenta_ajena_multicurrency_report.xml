        <record id="reporte_cuenta_ajena_multicurrency" model="ir.actions.report">
            <field name="name">Cuenta Ajena Multimoneda</field>
            <field name="model">account.move</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">adroc_shipment_fixes.cuenta_ajena_multicurrency_pdf</field>
            <field name="report_file">adroc_shipment_fixes.cuenta_ajena_multicurrency_pdf</field>
